<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.mosn.zdepthshadowsample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="app.mosn.zdepthshadowsample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleZDepthActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:name=".ChangeZDepthActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".WithRippleEffectActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".SampleFloatingActionMenuActivity"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>
